<!DOCTYPE html>
<head>
<title>WebCampBackendNew</title>


</head>
body>
<h2>Books</h2>


<table>

<thead>
<tr>
<th>Title</th>
<th>Body</th>
</tr>
</thead>


</table>


<% @books.each do |book| %>
<%= link_to "Show", book_path(book.id) %></br>
<%= link_to "Edit", edit_book_path(book.id.edit) %></br>
<% end %>

<h2>New Book</h2>
<!--form_with部分-->
<%= form_with model: Book.new do |f| %>
<h6>Title</h6>
<%=f.text_field:title %>

<h6>Body</h6>
<%=f.text_area:body %>
<div class="actions">
<%=f.submit'Create Book' %>
<% end %>
</div>
